{
  "name": "wolkenkit-template-chat",
  "version": "1.0.0",
  "description": "wolkenkit-template-chat is an application template for wolkenkit.",
  "wolkenkit": {
    "application": "chat",
    "runtime": {
      "version": "2.0.0"
    },
    "environments": {
      "default": {
        "api": {
          "address": {
            "host": "local.wolkenkit.io",
            "port": 3000
          },
          "allowAccessFrom": "*"
        },
        "node": {
          "environment": "development"
        }
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/thenativeweb/wolkenkit-template-chat.git"
  },
  "keywords": [
    "wolkenkit"
  ],
  "license": "AGPL-3.0",
  "dependencies": {
    "google-protobuf": "^3.6.1",
    "grpc-web-client": "^0.6.3",
    "tslib": "^1.9.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/google-protobuf": "^3.2.7",
    "@types/node": "8.10.30",
    "config-cev-generator": "^0.1.6",
    "http-server": "^0.11.1",
    "ts-node": "^7.0.1",
    "ts-protoc-gen": "^0.7.6",
    "typescript": "^3.0.3"
  }
}
