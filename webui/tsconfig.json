{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "esnext.asynciterable",
      "dom"
    ],
    "paths": {
      "@jchptf/ngx-web3": [
        "dist/jchptf/ngx-web3"
      ],
      "@jchptf/ngx-web3/*": [
        "dist/jchptf/ngx-web3/*"
      ],
      "@jchptf/ngx-random-art": [
        "dist/jchptf/ngx-random-art"
      ],
      "@jchptf/ngx-random-art/*": [
        "dist/jchptf/ngx-random-art/*"
      ],
      "@jchptf/ngx-message-channels": [
        "dist/jchptf/ngx-message-channels"
      ],
      "@jchptf/ngx-message-channels/*": [
        "dist/jchptf/ngx-message-channels/*"
      ]
    }
  },
  "exclude": [
    "migration/**/*"
  ]
}