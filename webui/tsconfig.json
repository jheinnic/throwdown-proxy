{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "esnext",
      "dom"
    ],
    "paths": {
      "@jchptf/ngx-web3": [
        "dist/jchptf/ngx-web3"
      ],
      "@jchptf/ngx-web3/*": [
        "dist/jchptf/ngx-web3/*"
      ]
    }
  }
}